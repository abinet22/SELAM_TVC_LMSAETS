<html lang="en"><head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>SELAM </title>
  
    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&amp;display=fallback">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="../../plugins/fontawesome-free/css/all.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="../../dist/css/adminlte.min.css">
  
  </head>
  <body>
  <div class="wrapper">
    <!-- Main content -->
    <section class="invoice">
      <!-- title row -->
      <div class="col-md-12" id="tobebilled">
        <div class="card card-primary">
         
          <div class="card-body">
            <div class="row">
              <div class="col-12">
                <table style="
             
                border-collapse: collapse;
                width: 100%;
              ">
                <tr style="
              
                border-collapse: collapse;
              ">
                  <td><img src="../../../dist/img/logoselam.jpg" width="150px" height="150px"></td>
                  <td colspan="2" style="
             
                  border-collapse: collapse;
                ">
                    <h4>
                      Organization Name <br>
                      Selam Technical And Vocational Collage
                     
                    </h4>
                  </td>
                 
                  <td  colspan="2"  style="
            
                  border-collapse: collapse;
                ">
    
                    <h5> <small class="float-">Doc No._____</small><br>
                      <small class="float-">SDR-TVC/QF/____</small></h5>
                      <small class="float-">Issued Date:<% var today = new Date().toLocaleDateString() %> <%= today %></small>
                  </td>
                </tr>
                <tr>
                  <td></td>
                  <td colspan="3">
                    <h5> <small class="">Office Of Registrar</small>
                      <small class="">Student Transcript</small></h5>
                  </td>
                
                  <td>
                    Issue No. __
                  </td>
                </tr>
              </table>
             
              </div>
              <!-- /.col -->
            </div>
           
         <div class="row">
            <div class="col-md-12">
                
                <div class="callout callout-success">
                    <div class="row">
                        <% if(marklist.length >0 ) { %>
                            <% var i = 0 ;%>
                               <% marklist.forEach(function (row){ %>
                                <% var name = JSON.parse(JSON.stringify(row.personal_info)) %>
                         
                        <div class="col-lg-12 ">
                            <div class="">
                                <div class=" p-1-9 p-sm-2-3 p-md-6 p-lg-7">
                                    <div class="row align-items-center">
                                      <table style="width: 100%;">
                                        <tr>
                                          <td>
                                            <div class="col-lg-6 mb-4 mb-lg-0">
                                              <%  dataphotos = 	'data:image/png;base64,' + new Buffer(row.applicant_photo, 'binary').toString('base64')
                                              %>
                                              <img class="img-fluid" id="appphoto"  src="<%= dataphotos %>" height="250px" width="250px" alt="User profile picture">
                   
                                          </div>
                                          </td>
                                          <td>
                                            <div class="col-lg-6 px-xl-10">
                                              <div class=" d-lg-inline-block py-1-9 px-1-9 px-sm-6 mb-1-9 " style="width: 100%; background-color:grey ;">
                                                  <h3 class="h2 text-white mb-0 " style="text-transform: uppercase;">Name: &nbsp;<%= name.firstname  %>&nbsp;<%= name.middlename  %></h3>
                                            
                                              </div>
                                              <ul class="list-unstyled mb-1-9">
                                                  <li class="mb-1  display-28"><span class="display-26 text-secondary me-2 font-weight-600">Sex:<%var gen =  name.gender %><%= gen=="m"?"Male":"Female" %></span> </li>
                                                  <li class="mb-1  display-28"><span class="display-26 text-secondary me-2 font-weight-600">Birth Date:</span> <%= name.birthday %></li>
                                                  <li class="mb-1  display-28"><span class="display-26 text-secondary me-2 font-weight-600">Age:</span> <% 
                                                     var birthday = name.birthday
                                                
                                                          var ageDifMs = Date.now() - new Date(birthday);
                                                          var ageDate = new Date(ageDifMs); 
                                                          var ageis =  Math.abs(ageDate.getUTCFullYear() - 1970);
                                                        
                                                      %> <%= ageis   %></li>
                                                  <li class="display-28"><span class="display-26 text-secondary me-2 font-weight-600">ID No:</span> <%= row.student_unique_id %></li>
                                              </ul>
                                             
                                          </div>
                                          </td>
                                        </tr>
                                      </table>
                                       
                                      
                                    </div>
                                </div>
                            </div>
                            <ul class="social-icon-style1 list-unstyled mb-0 ps-0">
                                <li class="mb-1  display-28"><span class="display-26 text-secondary me-2 font-weight-600">Year Of Entry:<%= batch.batch_from %></span><span class="display-26 text-secondary me-2 font-weight-600">Year Of Graduation:<% var dt =  new Date() %><%= dt.toLocaleString() %></span>  </li>
                                <li class="mb-1  display-28">
                                  <% if(sectorlist.length >0) {%>
                                    <% sectorlist.forEach(function (row){ %>
                                      <span class="display-26 text-secondary me-2 font-weight-600">Sector:<%= row.sector_name %></span>
                                  
                                  <span class="display-26 text-secondary me-2 font-weight-600">Department:<%= row.department_name %></span> 
                                  <span class="display-26 text-secondary me-2 font-weight-600">Occupation:<%= row.occupation_name %></span>  </li>
                             
                                   <% })}else {}%>
                                   <span class="display-26 text-secondary me-2 font-weight-600">Level:<%=row.current_level %> </span>
                                 <li class="mb-1 display-28"><span class="display-26 text-secondary me-2 font-weight-600">Admission Type:<%= row.admission_type %></span> </li>
                               
                               
                            </ul>
                        </div>
                  
                         <%  })} else{}     %>                           
                   
                  </div>
            </div>
           
        </div>
            <div class="row">
              <div class="col-md-12  grid-margin stretch-card">     <div class="card">
                 
                  <div class="card-body">
                  
                    <div class="row" style="display: none;">
                        <div class="col-sm-12 col-md-6">
                            <div class="dt-buttons btn-group flex-wrap">               
                        <button class="btn btn-secondary buttons-copy buttons-html5" tabindex="0" aria-controls="example1" type="button"><span>Copy</span></button> 
                        <button class="btn btn-secondary buttons-csv buttons-html5" tabindex="0" aria-controls="example1" type="button"><span>CSV</span></button> 
                        <button class="btn btn-secondary buttons-excel buttons-html5" tabindex="0" aria-controls="example1" type="button"><span>Excel</span></button> 
                        <button class="btn btn-secondary buttons-pdf buttons-html5" tabindex="0" aria-controls="example1" type="button"><span>PDF</span></button>
                         <button class="btn btn-secondary buttons-print" tabindex="0" aria-controls="example1" type="button"><span>Print</span></button> <div class="btn-group">
                             <button class="btn btn-secondary buttons-collection dropdown-toggle buttons-colvis" tabindex="0" aria-controls="example1" type="button" aria-haspopup="true" aria-expanded="false"><span>Column visibility</span></button></div> </div></div>
                             
                             <div class="col-sm-12 col-md-6"><div id="example1_filter" class="dataTables_filter"><label>Search:<input type="search" class="form-control form-control-sm" placeholder="" aria-controls="example1"></label></div></div>
                            </div>
                    <div class="table-responsive" id="trainingdata">
                        <table aria-describedby="example1_info" role="grid" class="table table-bordered table-striped dataTable dtr-inline" id="example1">
                                
                        <thead>
                          <tr>
                            <th>#</th>
                            <th>Unit Of Competency  </th>
                            <th>Module</th>
                            <th>Total Houres </th>
                                                       
                            <th>Assessement Result 100% </th>
                            <th>Grade In Letter</th>
                            <th>Grade In Point</th>
                         
                          </tr>
                        </thead>
                        <tbody>
                        <% if(courselist.length >0){ %>
                            <% courselist.forEach(function (row){ %>
                                <% var courseid = row.course_id %>
                                <% var coursecode = row.course_code %>
                                <% var coursename = row.course_name %>
                                <% var coursehour = row.training_hours %>
                                <% if(marklist.length >0 ) { %>
                                    <% var i = 0 ;%>
                                       <% marklist.forEach(function (row){ %>
                                        <% i++ %>
                                        <% var name = JSON.parse(JSON.stringify(row.personal_info))%>
                                        <% var course = row.course_id %>
                                         <% if(courseid == course){ %>
                                            <tr>
                                                <td><%= i %></td>
                                                <td ><%= coursename %>
                                             
                                                </td>
                                                <td><%= coursecode  %></td>
                                                
                                                <td><%= coursehour %></td>
                                              
                                                <td><%= row.total_result %> </td>
                                                <td><%= row.grade_in_latter %> </td>
                                                <td> <%= row.grade_in_point %></td>
                                           
                                            </tr>
                                         <% } %>
                                       
                                     <%  })} else{}     %>

                                <% })}else{}%> 
                        
                        
                             
                        </tbody>
                      </table>
                      <table style="width: 100%;">
                        <tr>
                          <th></th>
                          <th> Cumulative GPA </th>
                          <th>-</th>
                          <th> -</th>
                                                     
                          <th>-</th>
                          <th>-</th>
                          <th>4</th>
                       
                        </tr>
                      </table>
                    </div>
                  
                  </div>
                </div>
              </div>
           
          
            </div>
            <br>
            <hr>
            <br>
            <table>
              <tr>
            <td>Head Of Registrar</td>
            <td>_________________</td>
            <td>Collage Vice Dean</td>
            <td>_________________</td>
              </tr>
              
              <tr>
                <td>Sign</td>
            <td>_________________</td>
            <td>Sign</td>
            <td>_________________</td>
              </tr>
              
              <tr>
                <td>Date</td>
            <td>_________________</td>
            <td>Date</td>
            <td>_________________</td>
              </tr>
            </table>
            <br>
            <hr>

          </div>
        </div>
    
       
   
            

               
    </div>
   
      </div>

      <div class="row no-print">
        <div class="col-12">
          
          <a type="button " class="btn-block btn-sm btn no-print" style="background-color: aqua;" onclick="printDiv('tobebilled')"><i class="fa fa-print t-plus-1 fa-fw fa-lg"></i> Print Grade Report</a>
         <% if( programtag == "ngo"){ %>
          <a type="button " class="btn-block btn-sm btn no-print" style="background-color: aqua;" href="/registrardataencoder/trainee/managengobased"> Go Back</a>
       
          <% }else if(programtag == "level"){ %>
            <a type="button " class="btn-block btn-sm btn no-print" style="background-color: aqua;" href="/registrardataencoder/trainee/managelevelbased"> Go Back</a>
       
            <% }else if(programtag == "industry"){ %>
              <a type="button " class="btn-block btn-sm btn no-print" style="background-color: aqua;" href="/registrardataencoder/trainee/manageindustrybased"> Go Back</a>
       
              <% } %>
        
        </div>
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->
  </div>
  <!-- ./wrapper -->
  <!-- Page specific script -->
  <script>
    function printDiv(divName) {
        var printContents = document.getElementById(divName).innerHTML;
        var originalContents = document.body.innerHTML;
   
        document.body.innerHTML = printContents;
   
        window.print();
   
        document.body.innerHTML = originalContents;
   }
     </script>
  
  
  </body></html>
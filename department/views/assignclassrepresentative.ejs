      <div class="container-fluid">
          <div class="card card-primary ">
              <div class="card-header">
                <h3 class="card-title">
                  <i class="fas fa-edit"></i>
                 Select Training Program To Assign Class Representative
                </h3>
              </div>
              <div class="card-body">
                <h4>Program</h4>
                <div class="row">
                  <div class="col-5 col-sm-3">
                    <div class="nav flex-column nav-tabs h-100" id="vert-tabs-tab" role="tablist" aria-orientation="vertical">
                      <a class="nav-link" id="vert-tabs-home-tab" data-toggle="pill" href="#vert-tabs-home" role="tab" aria-controls="vert-tabs-home" aria-selected="false">Level Based</a>
                      <a class="nav-link" id="vert-tabs-profile-tab" data-toggle="pill" href="#vert-tabs-profile" role="tab" aria-controls="vert-tabs-profile" aria-selected="false">Short Term/Project Based/</a>
                      <a class="nav-link" id="vert-tabs-profilei-tab" data-toggle="pill" href="#vert-tabs-profilei" role="tab" aria-controls="vert-tabs-profilei" aria-selected="false">Industry Based</a>
                
                  </div>
                  </div>
                  <div class="col-7 col-sm-9">
                    <div class="tab-content" id="vert-tabs-tabContent">
                      <div class="tab-pane text-left fade" id="vert-tabs-home" role="tabpanel" aria-labelledby="vert-tabs-home-tab">
                        <form action="/department/teacher/assignclassrepresentativelevel" method="post">
                        
                          <div class="form-group">
                            <label>Select Level Based Program</label>
                            <select style="width: 100%;"  class="form-control-sm js-example-basic-single " name="programidlevel"  id="programidlevel">
                             <option value="0">Please Select Program/Batch Name</option>
                             <% if(levelbased.length >0) {%>
                                <% levelbased.forEach(function (row){ %>
                                  <option value="<%= row.batch_id %>"><%= row.batch_name %></option>
                               <% })}else {}%>
                            </select>
                          
                        </div>
                        <div class="form-group">
                          <label>Select Occupation </label>
                          <select style="width: 100%;"  class="form-control-sm js-example-basic-single " name="occupationname"  id="occupationname">
                           <option value="0">Please Select Occupation Name</option>
                           <% if(occupation.length >0) {%>
                              <% occupation.forEach(function (row){ %>
                                <option value="<%= row.occupation_id %>"><%= row.occupation_name %></option>
                             <% })}else {}%>
                          </select>
                        
                      </div>
                          <div class="form-group">
                              <label class="">Select Training Level </label>
                              <select class="form-control" name="traininglevel" id="traininglevel" onchange="showcriterias(this);">
                              <option value="0">Please Select Training Level</option>
                             
                               <option>Level_1</option>
                               <option>Level_2</option>
                               <option>Level_3</option>
                               <option>Level_4</option>
                               <option>Level_5</option>
                            
                              </select>
  
                               </div>
                               <div class="form-group">
                                <label class="">Select Program Type </label>
                                <select class="form-control" name="programtype" id="programtype" onchange="showcriterias(this);">
                                <option value="0">Please Select Program Type</option>
                                <option>Regular</option>
                                 <option>Extension</option>
                              
                                </select>
    
                                 </div>
                          <div class="col-md-6 offset-md-3">
                              <button type="submit" class="btn btn-block btn-sm btn-primary">Submit</button>
                   
                          </div>  
                      </form>
                         
                      </div>
                      <div class="tab-pane fade" id="vert-tabs-profile" role="tabpanel" aria-labelledby="vert-tabs-profile-tab">
                          <form action="/department/teacher/assignclassrepresentativengo" method="post">
                              <div class="form-group">
                                  <label>Select Short Term/Project Based/ Program</label>
                                  <select style="width: 100%;"  class="form-control-sm js-example-basic-single " name="programidn" id="programidn">
                                   <option value="0">Please Select Program/Batch Name</option>
                                   <% if(ngobased.length >0) {%>
                                      <% ngobased.forEach(function (row){ %>
                                        <option value="<%= row.batch_id %>"><%= row.batch_name %></option>
                                     <% })}else {}%>
                                  </select>
                                
                              </div>
                              <div class="form-group">
                                <label>Select Occupation </label>
                                <select style="width: 100%;"  class="form-control-sm js-example-basic-single " name="occupationnamen"  id="occupationnamen">
                                 <option value="0">Please Select Occupation Name</option>
                                 <% if(occupation.length >0) {%>
                                    <% occupation.forEach(function (row){ %>
                                      <option value="<%= row.occupation_id %>"><%= row.occupation_name %></option>
                                   <% })}else {}%>
                                </select>
                              
                            </div>
                              <div class="form-group">
                                  <label class="">Select Program Type </label>
                                  <select class="form-control" name="programtype" id="programtype" onchange="showcriterias(this);">
                                  <option value="0">Please Select Program Type</option>
                                  <option>Regular</option>
                                   <option>Extension</option>
                                   <option value="Short_Term">Short Term</option>
                                
                                  </select>
      
                                   </div>
                              <div class="col-md-6 offset-md-3">
                                  <button type="submit" class="btn btn-block btn-sm btn-primary">Submit</button>
                       
                              </div>
                              </form>
                        </div>
                        <div class="tab-pane fade" id="vert-tabs-profilei" role="tabpanel" aria-labelledby="vert-tabs-profilei-tab">
                          <form action="/department/teacher/assignclassrepresentativeindustry" method="post">
                              <div class="form-group">
                                  <label>Select Industry Based Program</label>
                                  <select style="width: 100%;"  class="form-control-sm js-example-basic-single " name="programidi" id="programidi">
                                   <option value="0">Please Select Program/Batch Name</option>
                                   <% if(industrybased.length >0) {%>
                                      <% industrybased.forEach(function (row){ %>
                                        <option value="<%= row.batch_id %>"><%= row.batch_name %></option>
                                     <% })}else {}%>
                                  </select>
                                
                              </div>
                              <div class="form-group">
                                <label>Select Occupation </label>
                                <select style="width: 100%;"  class="form-control-sm js-example-basic-single " name="occupationnamei"  id="occupationnamei">
                                 <option value="0">Please Select Occupation Name</option>
                                 <% if(occupation.length >0) {%>
                                    <% occupation.forEach(function (row){ %>
                                      <option value="<%= row.occupation_id %>"><%= row.occupation_name %></option>
                                   <% })}else {}%>
                                </select>
                              
                            </div>
                              <div class="form-group">
                                  <label class="">Select Program Type </label>
                                  <select class="form-control" name="programtype" id="programtype" onchange="showcriterias(this);">
                                  <option value="0">Please Select Program Type</option>
                                  <option>Regular</option>
                                   <option>Extension</option>
                                   <option value="Short_Term">Short Term</option>
                                
                                  </select>
      
                                   </div>
                              <div class="col-md-6 offset-md-3">
                                  <button type="submit" class="btn btn-block btn-sm btn-primary">Submit</button>
                       
                              </div>
                              </form>
                        </div>

  <script>
        var test = '<%- JSON.stringify(classwithevluation) %>'; 
        var data = JSON.parse(test)
        var xValues = [];
    var yValues = [];
        data.forEach(item =>{
         xValues.push(item.class_name);
         yValues.push(item.total)
        });
   console.log(data)
    var barColors = ["red", "green","blue","orange","brown","red", "green","blue","orange","brown","red", "green","blue","orange","brown"];
    
    new Chart("myChart", {
      type: "bar",
      data: {
        labels: xValues,
        datasets: [{
          backgroundColor: barColors,
          data: yValues
        }]
      },
      options: {
        legend: {display: false},
        title: {
          display: true,
          text: "Class Evaluation"
        }
      }
    });
    </script>
  
<script>
    var xValues = [];
    var test = '<%- JSON.stringify(classattendance) %>'; 
        var data = JSON.parse(test)
        var absent = [];
        var present = [];
        var permission = [];
    var yValues = [];
        data.forEach(item =>{
         xValues.push(item.class_name);
         absent.push(item.absent);
         present.push(item.present)
         permission.push(item.permission)
        });
    new Chart("myChart2", {
      type: "line",
      data: {
        labels: xValues,
        datasets: [{ 
          data: absent,
          borderColor: "red",
          fill: false
        }, { 
          data: present,
          borderColor: "green",
          fill: false
        }, { 
          data: permission,
          borderColor: "blue",
          fill: false
        }]
      },
      options: {
        legend: {display: false}
      }
    });
    </script>
  

  function printForm() {
   printJS({
       printable: 'costtable',
       type: 'html',
       targetStyles: ['*'],
       
       header: '<h3 class="custom-h3">Calculated Cost Statistics Data</h3>',
		style: '.custom-h3 { color: red; }'
   })
}

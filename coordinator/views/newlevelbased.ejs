    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <div class="row">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                      <div class="card-body">
                        <h4 class="card-title">Open Level Based Training Program </h4>
                      <br>
                          <ul class="nav nav-tabs " id="myTab" role="tablist">
                            <li class="nav-item" role="presentation">
                              <button class="nav-link  active" style="background-color: #f48d5b;" id="home-tab" data-bs-toggle="tab" data-bs-target="#home" type="button" role="tab" aria-controls="home" aria-selected="true">New Level Based Program Batch</button>
                            </li>
                         
                        
                          </ul>
                          <div class="tab-content " id="myTabContent">
                              
                            <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">    <div class="row">
                                <div class="row" style="display: none;">
                                    <div class="col-sm-12 col-md-6">
                                        <div class="dt-buttons btn-group flex-wrap">               
                                    <button class="btn btn-secondary buttons-copy buttons-html5" tabindex="0" aria-controls="example1" type="button"><span>Copy</span></button> 
                                    <button class="btn btn-secondary buttons-csv buttons-html5" tabindex="0" aria-controls="example1" type="button"><span>CSV</span></button> 
                                    <button class="btn btn-secondary buttons-excel buttons-html5" tabindex="0" aria-controls="example1" type="button"><span>Excel</span></button> 
                                    <button class="btn btn-secondary buttons-pdf buttons-html5" tabindex="0" aria-controls="example1" type="button"><span>PDF</span></button>
                                     <button class="btn btn-secondary buttons-print" tabindex="0" aria-controls="example1" type="button"><span>Print</span></button> <div class="btn-group">
                                         <button class="btn btn-secondary buttons-collection dropdown-toggle buttons-colvis" tabindex="0" aria-controls="example1" type="button" aria-haspopup="true" aria-expanded="false"><span>Column visibility</span></button></div> </div></div>
                                         
                                         <div class="col-sm-12 col-md-6"><div id="example1_filter" class="dataTables_filter"><label>Search:<input type="search" class="form-control form-control-sm" placeholder="" aria-controls="example1"></label></div></div></div>
                                <div class="col-md-12  grid-margin stretch-card">     <div class="card">
                                  <div class="card-body">
                                  <div class="table-responsive" >
                                        <table aria-describedby="example1_info" role="grid" class="table table-bordered table-striped dataTable dtr-inline" id="example1">
                                          <thead>
                                            <tr>
                            <th>#</th>
                                              <th>Batch Name</th>
                                              <th>Program Description</th>
                                              <th>Training Name</th>
                                              <th>Training Level</th>
                                              <th>Training Type</th>
                                              <th> Training From-To</th>
                                              <th> Application From-To</th>
                                              <th> Minimum Requirements </th>
                                              <th> Applicant Selection Criteria  </th>
                                              <th> Application Deadline  </th>
                                              <th> Register Applicant </th>
                                             
                                            </tr>
                                          </thead>
                                          <tbody>
                          
                                             
                                          
                                           <% if(levelbased.length >0 ) { %>
                                            <% var r = 0 ;%>
                                               <% levelbased.forEach(function (row){ %>
                                                <% r++ %>
                                                <% var asd = new Date(row.app_start_date).toLocaleDateString(); %> 
                                                <% var aed = new Date(row.app_end_date).toLocaleDateString();   %> 
                                              
                                                <% var tsd = new Date(row.training_start_date ).toLocaleDateString();%> 
                                                 <% var ted = new Date(row.training_end_date).toLocaleDateString();  %> 
                                                 <% var todayis = new Date().toLocaleDateString();%>
                                                <tr>
                                                 
                                                    <td><%= r %></td>
                                                    <td><%= row.batch_name %></td>
                                                    <td><%= row.program_description %></td>
                                                    <td><% var trainingnames =[];
                                                        var abc ;
                                                        trainingnames  =  JSON.parse(row.training_names);
                                                     
                                                          %>
                                                          <% for(var j =0 ; j< trainingnames.length ;j++) { %>
                                                            <% var nameid =  trainingnames[j]; %>
                                                          
                                                            <label class="form-check-label">
                                                                <input type="checkbox" class="form-check-input" checked="">
                                                                <%= trainingnames[j] %>
                                                              <i class="input-helper"></i></label>
                                                       <% } %>
                                                       
                                                       
                                                        </td>
                                                        <td><% var traininglevels =[];
                                                          traininglevels  =  JSON.parse(row.training_levels);
                                                          %>
                                                          <% for(var j =0 ; j< traininglevels.length ;j++) { %>
                                                          <label class="form-check-label">
                                                            <input type="checkbox" class="form-check-input" checked="">
                                                            <%= traininglevels[j] %>
                                                          <i class="input-helper"></i></label>
                                                       <% } %>
                                                        </td>
                                                        <td><% var trainingtype =[];
                                                          trainingtype  =  JSON.parse(row.training_types);
                                                          %>
                                                          <% for(var j =0 ; j< trainingtype.length ;j++) { %>
                                                          <label class="form-check-label">
                                                            <input type="checkbox" class="form-check-input" checked="">
                                                            <%= trainingtype[j] %>
                                                          <i class="input-helper"></i></label>
                                                       <% } %>
                                                        </td>
                                               
                                                    <td><%= tsd +"-"+ ted %></td>
                                                    <td><%= asd +"-"+ aed %></td>
                                                    <td><%= row.minimum_criteria %> </td>
                                                    <td><% var selectioncriteria= [];
                                                     var x =[];
                                                     var objs = JSON.parse(row.selection_criteria);
                                                     selectioncriteria.push(objs);
                                                     selectioncriteria.forEach(obj => {
                                                       Object.entries(obj).forEach(([key, value]) => {
                                                          var key  = key;
                                                          var value =value;
                                                          x.push(key+"="+value+ "%");
                                                       });
                                                     
                                                   }); %>
                                                   
                                                   <% for(var i= 0 ; i < x.length ; i++ ) { %>
                                                     <label class="form-check-label">
                                                       <input type="checkbox" class="form-check-input" checked="">
                                                        <%= x[i] %>  
                                                     <i class="input-helper"></i></label>
                                                   <% } %>
                                                 
                                                 
                                                   </td>
                                                    <td> 
                                                      <% if(aed.valueOf() > todayis.valueOf()){ %> 
                                                     <form >
                                                     
                                                 <button class="btn btn-block btn-info">Not Passed</button>     
                                                     </form>
                                                   <% }else{%>
                                                     <form >
                                                       <button class="btn btn-block btn-danger">Passed</button>      
                                                     </form>
                                                   
                                                     <% }%>
                                                   </td>
                                    
                                                   <td> 
                                                    <% if(aed.valueOf() > todayis.valueOf()){ %> 
                                                      <form action="/registration/registerlevelbasedapplicant/<%= row.program_id %>" method="get">
                                                        <button class="btn btn-block btn-info"><i class="fa fa-edit"></i>Register Applicants</button> 
                                                    
                                                      </form>
                                                    <% }else{%>
                                                      <form >
                                                        <button class="btn btn-block btn-danger">Application End Date Passed</button>      
                                                      </form>
                                                    
                                                      <% }%>
                                                  
                                                 
                                                       </td>
                                                  </tr>
                                             <%  })} else{}     %>
                                            
                                        
                            
                                               
                                           
                                          
                                          </tbody>
                                        </table>
                                      </div>
                                  
                                  </div>
                                </div>
                              </div>
                           
                          
                            </div>
                          </div>
                          
                        
                        
                
                        </div>
                      </div>
                    </div>
                
                   
               
                        
        
                           
                        </div>
              </div>
          
          </div>
    
